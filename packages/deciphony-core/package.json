{
  "name": "deciphony-core",
  "version": "0.0.12",
  "description": "",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./enum": {
      "types": "./dist/musicScoreEnum.d.ts",
      "default": "./dist/musicScoreEnum.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js",
      "module": "./dist/utils/*.js",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.js"
    },
    "./utils/core": {
      "types": "./dist/utils/core/*.d.ts",
      "default": "./dist/utils/core/*.js",
      "module": "./dist/utils/core/*.js",
      "import": "./dist/utils/core/*.js",
      "require": "./dist/utils/core/*.js"
    },
    "./utils/musicScoreDataUtil": {
      "types": "./dist/utils/musicScoreDataUtil.d.ts",
      "default": "./dist/utils/musicScoreDataUtil.js"
    }
  },
  "dependencies": {},
  "keywords": [],
